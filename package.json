{
  "name": "maddevs",
  "version": "2.0.0",
  "description": "Mad Devs",
  "author": "Mad Devs",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "npm run lint-vue-scss & npm run lint-scss & npm run lint-es ",
    "lint-es": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint-vue-scss": "sass-lint-vue components",
    "lint-scss": "sass-lint client/assets/styles/*.scss --verbose",
    "precommit": "npm run lint",
    "heroku-postbuild": "npm run generate",
    "heroku-cleanup": "rm -rf client && rm -rf .nuxt",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "test": "jest --config=jest.config.develop.js",
    "test:coverage": "jest --config=jest.config.develop.js && node ./jest_config/jest-coverage-badges.js -output './client/static'",
    "test:staging": "jest --config=jest.config.staging.js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/prismic": "^1.2.4",
    "@nuxtjs/robots": "^2.4.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nuxt": "^2.15.0",
    "prismjs": "^1.23.0",
    "request": "^2.88.2",
    "sendpulse-api": "^1.1.4",
    "snyk": "^1.439.0",
    "swiper": "^5.4.5",
    "vee-validate": "^3.4.5",
    "vue-awesome-swiper": "^4.1.1",
    "vue-embed-gist": "^1.1.0",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-lottie": "^0.2.1",
    "vue-prism-editor": "^1.2.2",
    "vue-select": "^3.11.2",
    "vue-silentbox": "^2.3.2",
    "vue-social-sharing": "^3.0.4",
    "vue2-perfect-scrollbar": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-vue": "^4.0.0",
    "husky": "^4.3.0",
    "jest": "^25.5.4",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.14.1",
    "sass-lint": "^1.13.1",
    "sass-lint-vue": "^0.4.0",
    "sass-loader": "^7.3.1",
    "vue-jest": "^3.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "snyk": true
}
